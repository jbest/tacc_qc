{
    "versions": {
        "config_format": "3.0"
    },
    "collection": {
        "name": "BRIT",
        "prefix": "BRIT",
        "catalog_number_regex": "(?P<catNum>(?P<instID>BRIT)(-(?P<collID>XX)-)*(?P<numerical>\\d+))",
        "web_base_TACC": "/corral-repl/projects/TORCH/web/BRIT/",
        "web_base": "/media/jbest/data3/BRIT_git/tacc_qc/dir/",
        "url_base": "https://web.corral.tacc.utexas.edu/torch/"
    },
    "files": {
        "folder_increment": 1000,
        "number_pad": 7,
        "log_directory_path": "/media/jbest/data3/BRIT_git/tacc_qc/dir/",
        "output_base_path": "/media/jbest/data3/BRIT_git/tacc_qc/dir/",
        "input_path": "/corral-repl/projects/TORCH/staging/BRIT/ready_to_sort"
    },
    "file_types": {
        "web_jpg": {
            "file_regex": "(_(?P<suffix>.+))*(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/BRIT/"
        },
        "web_jpg_med": {
            "file_regex": "(_(?P<suffix>.+))*(_)(?P<size>med)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/BRIT/"
        },
        "web_jpg_thumb": {
            "file_regex": "(_(?P<suffix>.+))*(_)(?P<size>thumb)(\\.)(?i)(?P<ext>jpg|jpeg)",
            "output_sub_path": "web/BRIT/"
        },
        "archive_dng": {
            "file_regex": "(_(?P<suffix>.+))*(\\.)(?i)(?P<ext>dng)",
            "output_sub_path": "archive/BRIT/"
        },
        "ocr": {
            "file_regex": "(_(?P<suffix>.+))*(_ocr)(\\.)(?i)(?P<ext>txt|json)",
            "output_sub_path": "web/BRIT/"
        }
    }
}
